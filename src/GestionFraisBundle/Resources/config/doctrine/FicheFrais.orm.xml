<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="GestionFraisBundle\Entity\FicheFrais" table="fiche_frais">
    <indexes>
      <index name="idEtatFicheFrais" columns="idEtatFicheFrais"/>
      <index name="idVisiteur" columns="idVisiteur"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="mois" type="string" column="mois" length="6" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="datemodif" type="datetime" column="dateModif" nullable="false"/>
    <field name="montantvalide" type="float" column="montantValide" precision="10" scale="0" nullable="false"/>
    <many-to-one field="idvisiteur" target-entity="Visiteur" fetch="LAZY">
      <join-columns>
        <join-column name="idVisiteur" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idetatfichefrais" target-entity="EtatFicheFrais" fetch="LAZY">
      <join-columns>
        <join-column name="idEtatFicheFrais" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
